{
  "action_manifest_version": 0,
  "alias_info": {},
  "app_key": "steam.overlay.1009850",
  "bindings": {
    "/actions/haptic": {
      "haptics": [
        {
          "output": "/actions/haptic/out/hapticsleft",
          "path": "/user/hand/left/output/haptic"
        },
        {
          "output": "/actions/haptic/out/hapticsright",
          "path": "/user/hand/right/output/haptic"
        }
      ],
      "sources": [
        {
          "inputs": {},
          "mode": "none",
          "path": "/user/hand/left/input/y"
        },
        {
          "inputs": {},
          "mode": "none",
          "path": "/user/hand/right/input/b"
        }
      ]
    },
    "/actions/motion": {
      "sources": [
        {
          "inputs": {
            "click": {
              "output": "/actions/motion/in/lefthandspaceturn"
            }
          },
          "mode": "button",
          "path": "/user/hand/left/input/y"
        },
        {
          "inputs": {
            "click": {
              "output": "/actions/motion/in/swapspacedragtolefthandoverride"
            }
          },
          "mode": "button",
          "path": "/user/hand/left/input/y"
        },
        {
          "inputs": {
            "click": {
              "output": "/actions/motion/in/lefthandspacedrag"
            }
          },
          "mode": "button",
          "path": "/user/hand/left/input/x"
        },
        {
          "inputs": {
            "click": {
              "output": "/actions/motion/in/resetoffsets"
            }
          },
          "mode": "button",
          "path": "/user/hand/left/input/joystick"
        },
        {
          "inputs": {
            "click": {
              "output": "/actions/motion/in/moveforward"
            }
          },
          "mode": "button",
          "path": "/user/hand/left/input/trigger"
        }
      ]
    },
    "/actions/system": {
      "sources": [
        {
          "inputs": {
            "click": {
              "output": "/actions/system/in/pushtotalk"
            }
          },
          "mode": "button",
          "path": "/user/hand/left/input/trigger"
        }
      ]
    }
  },
  "category": "steamvr_input",
  "controller_type": "oculus_touch",
  "description": "",
  "name": "OVR Advanced Setting/Oculus Touch/左手操作設定",
  "options": {},
  "simulated_actions": []
}
